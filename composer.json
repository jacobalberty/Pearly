{
    "name": "jacobalberty/pearly",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/jacobalberty/php-closure-compiler.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Pearly\\":         "class/Pearly"
        },
        "classmap": [
            "class/Pearly/Http.php",
            "class/Pearly/Html.php"
        ],
        "files": [ "includes/functions.inc.php" ]
    },
    "require": {
        "itbz/fpdf": "^1.7",
        "psr/log": "^1.0",
        "tureki/phpcc": "dev-sortfix"
    }
}
